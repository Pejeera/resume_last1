# Application Configuration
USE_MOCK=true
DEBUG=false

# CORS
CORS_ORIGINS=["*"]

# AWS Configuration
AWS_REGION=ap-southeast-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# S3 Configuration
S3_BUCKET_NAME=resume-matching-bucket
S3_PREFIX=resumes/

# OpenSearch Configuration
OPENSEARCH_ENDPOINT=https://your-opensearch-endpoint.us-east-1.es.amazonaws.com
OPENSEARCH_USERNAME=admin
OPENSEARCH_PASSWORD=your_password_here
OPENSEARCH_USE_SSL=true
OPENSEARCH_VERIFY_CERTS=false

# Bedrock Configuration
BEDROCK_REGION=ap-southeast-1
BEDROCK_EMBEDDING_MODEL=cohere.embed-multilingual-v3
BEDROCK_RERANK_MODEL=us.amazon.nova-lite-v1:0

# Secrets Manager (Optional)
SECRETS_MANAGER_SECRET_NAME=resume-matching-secrets

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

