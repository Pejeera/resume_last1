{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowListResumeBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::resume-matching-533267343789"
    },
    {
      "Sid": "AllowReadResumeObjects",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": "arn:aws:s3:::resume-matching-533267343789/*"
    },
    {
      "Sid": "AllowWriteResumeObjects",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource": "arn:aws:s3:::resume-matching-533267343789/*"
    }
  ]
}

